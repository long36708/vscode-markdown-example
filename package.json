{
  "name": "example-for-markdown",
  "displayName": "Example For Markdown",
  "description": "Full Markdown Example",
  "version": "0.0.1",
  "icon": "markdown.png",
  "publisher": "longmo",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
     "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
      "commands": [{
          "command": "markdown.example",
          "title": "MarkdownExample"
      }],
      "menus": {
          "editor/context": [{
              "when": "resourceExtname == .md",
              "command": "markdown.example",
              "group": "navigation"
          }]
      }
    },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
